01|+     /// Adds two numbers together                                             
02|+     /// Multiplies two numbers                                                
03|+                                                                               
04|+     /// Divides two numbers with error handling                               
05|+     fn divide(a: i32, b: i32) -> Result<i32, &'static str> {                  
06|+         if b == 0 {                                                           
07|+             Err("Division by zero")                                           
08|+         } else {                                                              
09|+             Ok(a / b)                                                         
10|+         }                                                                     
11|+     }                                                                         
12|      }                                                                         
13|  }                                                                             
